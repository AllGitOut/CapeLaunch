# FloatPlan Marine Weather API Configuration
# Copy this file to .env and add your actual API keys

# Stormglass.io API Key for marine weather data
# Get your free API key at: https://stormglass.io/ (50 requests/day free)
VITE_STORMGLASS_API_KEY=your_stormglass_api_key_here

# Development Mode - Set to 'false' to use real API data, 'true' to use mock data
# Use 'true' during development to save API calls (only 10 requests/day limit)
VITE_USE_MOCK_DATA=true

# NOAA Station ID for tide data (default: Trident Pier, Cape Canaveral, FL)
# Find station IDs at: https://tidesandcurrents.noaa.gov/
VITE_NOAA_STATION_ID=8721604

# Location coordinates (default: Satellite Beach, FL)
VITE_LAT=28.1761
VITE_LON=-80.5901
